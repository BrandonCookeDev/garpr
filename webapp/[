var r = db.regionse.find();
r.forEach(region => {
	db.regions.update(
		{_id: region._id},
		{$set: {"state_code": ""} }
	)
})

r.forEach(region => {
	db.regions.update(
		{_id: region._id},
		{$set: {"subregions": []}}
	)
})

r.forEach(region => {
	db.regions.update(
		{_id: region._id},
		{$unset: {"state_code": "", "subregions": ""} }
	)
})



